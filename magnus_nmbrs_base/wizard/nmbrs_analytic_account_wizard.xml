<?xml version="1.0" encoding="utf-8"?>
<odoo>
  	<record id="mapping_nmbrs_analytic_account_wizard" model="ir.ui.view">
      <field name="name">nmbrs.analytic.account.wizard.form</field>
      <field name="model">nmbrs.analytic.account.wizard</field>
      <field name="arch" type="xml">
          <form string="Import Analytic Accounts from NMBRs">
              <group cols="8">
                <field name="operating_unit" style="width:16%"/>
              </group>
      		<footer>
                <button name="fetch_analytic_accounts_nmbrs" string="Fetch Analytic Accounts" type="object" class="btn-primary"/>
                <button string="Cancel" class="btn-default" special="cancel"/>
            </footer>
      </form>
      </field>
    </record>
</odoo>

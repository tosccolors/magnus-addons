<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="payroll_runs_form_wizard" model="ir.ui.view">
        <field name="name">nmbrs.payroll.runs.wizard.form</field>
        <field name="model">nmbrs.payroll.runs.wizard</field>
        <field name="arch" type="xml">
          <form string="Import Payroll Runs NMBRs">
                  <group cols="8">
                      <field name="operating_unit" style="width:16%"/>
                      <field name="year" style="width:16%"/>
                  </group>
<!--              <button name="fetch_payroll_runs_nmbrs" string="Fetch Payroll Runs" type="object" class="oe_highlight"/>-->
              <footer>
                <button name="fetch_payroll_runs_nmbrs" string="Fetch Payroll Runs" type="object" class="oe_highlight"/>
                <button string="Cancel" class="btn-default" special="cancel"/>
              </footer>

          </form>
        </field>
    </record>
</odoo>
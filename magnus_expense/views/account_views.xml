<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- this view is switched off in __manifest__-->
        <record id="view_account_analytic_line_tree_inherit_magnus_account" model="ir.ui.view">
            <field name="name">account.analytic.line.tree.inherit.magnus.account</field>
            <field name="model">account.analytic.line</field>
            <field name="inherit_id" ref="analytic.view_account_analytic_line_tree"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='unit_amount']" position="after">
                        <field name="customer_charge_expense" />
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>